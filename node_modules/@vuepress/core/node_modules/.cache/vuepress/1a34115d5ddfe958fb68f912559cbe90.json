div class=\"name\">{{ project.name }}</div>\r\n      <div class=\"desc\">{{ project.desc }}</div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script src=\"./ProjectList\" />\r\n\r\n<style lang=\"stylus\">\r\n.project-list\r\n  position relative\r\n  display flex\r\n  justify-content flex-start\r\n  align-content stretch\r\n  align-items stretch\r\n  flex-wrap wrap\r\n  font-family sans-serif\r\n  margin-bottom 12px\r\n  z-index 2\r\n\r\n  .project\r\n    position relative\r\n    width calc(50% - 40px)\r\n    background-color var(--grey14)\r\n    border-radius 8px\r\n    margin 6px 8px\r\n    padding 12px\r\n    transition background-color 0.3s, transform 0.3s\r\n\r\n    @media (min-width $MQNarrow)\r\n      width calc(33% - 40px)\r\n\r\n    @media (min-width $MQWide)\r\n      width calc(25% - 40px)\r\n\r\n    &:hover\r\n      cursor pointer\r\n      transform scale(0.98, 0.98)\r\n\r\n    .cover\r\n      content ''\r\n      opacity 0.5\r\n      top 0\r\n      left 0\r\n      bottom 0\r\n      right 0\r\n      position absolute\r\n      z-index 1\r\n\r\n    .icon\r\n      position relative\r\n      z-index 2\r\n      float right\r\n      width 20px\r\n      height 20px\r\n\r\n    .name\r\n      position relative\r\n      z-index 2\r\n      color var(--grey3)\r\n      font-size 16px\r\n      font-weight 500\r\n\r\n    .desc\r\n      position relative\r\n      z-index 2\r\n      margin 6px 0\r\n      color var(--dark-grey)\r\n      font-size 13px\r\n\r\n@require '~@mr-hope/vuepress-shared/styles/colors.styl'\r\n\r\nfor $color, $index in $colors\r\n  .project-list .project{$index}\r\n    &, .theme-light &\r\n      background lighten($color, 90%)\r\n\r\n      &:hover\r\n        background lighten($color, 75%)\r\n\r\n    .theme-dark &\r\n      background darken($color, 75%)\r\n\r\n      &:hover\r\n        background darken($color, 60%)\r\n</style>\r\n"]}]}